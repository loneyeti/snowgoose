# Rename this file to .env.local

# Database Configuration
# For local setup:
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/snowgoose"
# For Docker setup:
# DATABASE_URL="postgresql://postgres:postgres@db:5432/snowgoose"

# Supabase Authentication
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Google Cloud Storage Information
GCS_CLIENT_EMAIL=serviceaccountname@yourprojectname.iam.gserviceaccount.com
GCS_PROJECT_ID=gcprojectid
GCS_BUCKET_NAME=gcsbucketname
GCS_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nprivatekey\n-----END PRIVATE KEY-----\n"

# AI Service API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# MCP Configuration
MCP_CONFIG_PATH=/path/to/your/mcp/config
